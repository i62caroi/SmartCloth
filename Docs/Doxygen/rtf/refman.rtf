409             \par
410             tareScale();                    {\cf20 // Tara inicial}\par
411             \par
412             pesoRecipiente = 0.0;           {\cf20 // Se inicializa 'pesoRecipiente', que se sumar\'E1 a 'pesoPlato' para saber el 'pesoARetirar'.}\par
413             pesoPlato = 0.0;                {\cf20 // Se inicializa 'pesoPlato', que se sumar\'E1 a 'pesoRecipiente' para saber el 'pesoARetirar'.}\par
414             pesoLastAlimento = 0.0;         {\cf20 // Se inicializa 'pesoLastAlimento', que, si hubiera un \'FAltimo alimento que a\'F1adir en delete,}\par
415                                             {\cf20 // se sumar\'E1 a 'pesoPlato' y luego a 'pesoRecipiente' para saber el 'peroARetirar'.}\par
416             \par
417             pedirRecipiente();              {\cf20 // Pedir recipiente}\par
418         \}\par
419 \par
420         doneState = {\cf17 true};                   {\cf20 // Solo realizar una vez las actividades del estado por cada vez que se active y no}\par
421                                             {\cf20 // cada vez que se entre a esta funci\'F3n debido al loop de Arduino.}\par
422                                             {\cf20 // As\'ED, debe ocurrir una nueva transici\'F3n que lleve a este evento para que se "repitan"}\par
423                                             {\cf20 // las acciones.}\par
424     \}\par
