458 {\cf18 void} actGruposAlimentos()\{ \par
459     {\cf17 static} {\cf18 unsigned} {\cf18 long} previousTime;     {\cf20 // Tiempos utilizados para regresar al STATE_Empty (pedir recipiente) si pasan 5 segundos sin}\par
460     {\cf18 unsigned} {\cf18 long} currentTime;             {\cf20 // colocar alimento y previamente no se coloc\'F3 recipiente, sino que se escogi\'F3 grupo directamente. }\par
461                                            {\cf20 // El "temporizador" de 5 segundos se reinicia cada vez que se escoge un grupo nuevo.}\par
462 \par
463     {\cf19 if}(!doneState)\{        \par
464 \par
465         previousTime = millis();           {\cf20 // Reiniciar "temporizador" de 5 segundos para regresar a STATE_Empty y pedir recipiente.}\par
466         \par
467         {\cf20 // ----- ACCIONES ------------------------------}\par
468         {\cf19 if}(state_prev == STATE_Plato)\{                                     {\cf20 // ==> Si se viene de STATE_Plato porque se acaba de colocar el recipiente.}\par
469             pesoRecipiente = pesoBascula;                                   {\cf20 // Se guarda 'pesoRecipiente' para sumarlo a 'pesoPlato' y saber el 'pesoARetirar'.}\par
470             {\cf20 //tareScale();                                                    // Tarar para tomar peso real del alimento que se va a colocar.}\par
471         \}\par
472         {\cf19 else} {\cf19 if}(pesoBascula != 0.0)\{ \par
473                 Serial.println(F({\cf22 "A\'F1adiendo alimento al plato..."}));\par
474                 \par
475                 Alimento alimento(grupoAnterior, pesoBascula);                {\cf20 // C\'E1lculo autom\'E1tico de valores nutricionales.}\par
476                                                                             {\cf20 // Al escoger un nuevo grupo se guarda el alimento del grupo anterior}\par
477                                                                             {\cf20 // colocado en la iteraci\'F3n anterior. Por eso se utiliza 'grupoAnterior' para}\par
478                                                                             {\cf20 // crear el alimento, porque 'grupoEscogido' ya ha tomado el valor del}\par
479                                                                             {\cf20 // nuevo grupo.}\par
480                                                              \par
481                 platoActual.addAlimentoPlato(alimento);                               {\cf20 // Alimento ==> Plato}\par
482                 comidaActual.addAlimentoComida(alimento);                             {\cf20 // Alimento ==> Comida}\par
483 \par
484                 pesoPlato = platoActual.getPesoPlato();                     {\cf20 // Se actualiza el 'pesoPlato' para sumarlo a 'pesoRecipiente' y saber el 'pesoARetirar'.}\par
485                 \par
486         \}\par
487         {\cf20 // ----- FIN ACCIONES --------------------------}\par
488 \par
489 \par
490         {\cf20 // ----- INFO PANTALLA -------------------------}\par
491         {\cf19 if}((state_prev != STATE_groupA) && (state_prev != STATE_groupB))\{ {\cf20 // ==> Si es la primera vez que se escoge grupo, se muestra todo (ejemplos, plato actual y acumulado)}\par
