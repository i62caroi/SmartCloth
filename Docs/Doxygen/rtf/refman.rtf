142 {\cf18 void} saveComidaSD()\{\par
143     Serial.println(F({\cf22 "Guardando info...\\n"}));\par
144 \par
145     {\cf20 // Se ha utilizado un RTC para conocer la fecha a la que se guarda la comida}\par
146 \par
147     {\cf20 // Debe separarse por ';' para que Excel abra el fichero csv separando las}\par
148     {\cf20 // columnas directamente:}\par
149     {\cf20 //    "fecha;hora;carb;carb_R;lip;lip_R;prot;prot_R;kcal;peso"}\par
150 \par
151     String comidaValues = comidaActual.getComidaAllValues(); {\cf20 // carb;carb_R;lip;lip_R;prot;prot_R;kcal}\par
152     String peso = String(comidaActual.getPesoComida());\par
153 \par
154     String dataString = String(rtc.getDateStr()) + {\cf22 ";"} + String(rtc.getTimeStr()) + {\cf22 ";"} + comidaValues + {\cf22 ";"} + peso; \par
155 \par
156 \par
157     myFile = SD.open(fileCSV, FILE_WRITE); {\cf20 // Todo se va a ir guardando en el mismo fichero ==> 'fileCSV' en Variables.h}\par
