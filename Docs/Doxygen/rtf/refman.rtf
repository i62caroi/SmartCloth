751 {\cf20 /*---------------------------------------------------------------------------------------------------------}\par
752 {\cf20    actStateDeleted(): Acciones del STATE_deleted}\par
753 {\cf20       !!!!!!!!!!!!!!!!!!!!!!!!!!!! IMPORTANTE !!!!!!!!!!!!!!!!!!!!!!!!!}\par
754 {\cf20 }\par
755 {\cf20         PARA EL DELETE RETROACTIVO, PRIMERO SE BORRA EL PLATO ACTUAL, SI TIENE ALGO, Y SE DEBE RETIRAR}\par
756 {\cf20         PARA PODER SEGUIR BORRANDO PLATOS. HASTA QUE NO SE LIBERE LA B\'C1SCULA NO SE PERMITIR\'C1 SEGUIR BORRANDO.}\par
757 {\cf20 }\par
758 {\cf20       !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!}\par
759 {\cf20 ----------------------------------------------------------------------------------------------------------*/}\par
760 {\cf18 void} actStateDeleted()\{\par
761     {\cf17 static} {\cf18 unsigned} {\cf18 long} previousTime;     {\cf20 // Tiempos utilizados para regresar a STATE_groupA / B si se ha querido borrar pero el plato }\par
762     {\cf18 unsigned} {\cf18 long} currentTime;             {\cf20 // estaba vac\'EDo. Se muestra un aviso y pasados 3 segundos se regresa al estado seg\'FAn el \'FAltimo grupo escogido.}\par
763 \par
764     {\cf17 static} {\cf18 bool} errorPlatoWasEmpty;        {\cf20 // Flag utilizada para saber si se debe mostrar la informaci\'F3n "normal"}\par
765                                            {\cf20 // o un mensaje de aviso indicando no se ha borrado el plato porque el }\par
766                                            {\cf20 // actual ya est\'E1 vac\'EDo.}\par
767 \par
768     {\cf19 if}(!doneState)\{\par
769 \par
770         {\cf20 // -------------------- DELETE ACTUAL ------------------------        }\par
771         {\cf19 if}(state_prev != STATE_deleted)\{                            {\cf20 // ==> Si no se viene del propio STATE_deleted, para evitar que se vuelva }\par
772                                                                     {\cf20 //     a eliminar el plato de la comida.}\par
773           \par
774             \par
775             Serial.println(F({\cf22 "\\nEliminando plato..."}));\par
776 \par
777             \par
778             {\cf20 /* ----- PESO \'DALTIMO ALIMENTO  ----- */}\par
779             {\cf19 if}(pesoBascula != 0.0)\{           {\cf20 // ==> Si se ha colocado algo nuevo en la b\'E1scula (pesoBascula marca algo) y no se ha retirado,}\par
780                                               {\cf20 //     debe incluirse en el plato. }\par
781 \par
782                                               {\cf20 // En este caso no se va a guardar el alimento porque se va a borrar el plato. Solamente se guardar\'E1 el 'pesoLastAlimento'}\par
783                                               {\cf20 // para a\'F1adirlo a 'pesoPlato' y luego, tras sumarlo al 'pesoRecipiente', saber el 'pesoARetirar'.}\par
784 \par
785                                               {\cf20 // Estando en cualquiera de los estados previos a STATE_weighted, si se decidiera }\par
786                                               {\cf20 // eliminar el plato actual sin haber colocado un nuevo alimento (pesoBascula == 0.0), }\par
787                                               {\cf20 // no har\'EDa falta actualizar el peso del plato, pues no se habr\'EDa modificado.}\par
788                                                          \par
789                 pesoLastAlimento = pesoBascula;    {\cf20 // Se guarda el 'pesoLastAlimento' para luego a\'F1adirlo a 'pesoPlato' y, tras sumarlo al 'pesoRecipiente', }\par
790                                                    {\cf20 // saber el 'pesoARetirar'.}\par
791 \par
792                 {\cf20 /* ----- TARAR  ----- */}\par
793                 tareScale();                       {\cf20 // Se debe tarar para que conforme vaya disminuyendo el peso veamos si se ha quitado todo el 'pesoARetirar'.}\par
794                                                    {\cf20 // Solo se hace si se viene del STATE_weighted porque en los estados previos a ese ya se tara y es en }\par
795                                                    {\cf20 // STATE_weighted donde se modifica el peso.}\par
796             \}\par
797 \par
798 \par
799             {\cf20 /* ----- BORRAR PLATO DE COMIDA  ----- */}\par
800             {\cf19 if}(!platoActual.isPlatoEmpty())\{    {\cf20 // PLATO CON COSAS ==> HAY QUE BORRAR Y RETIRAR (plato guardado o solo \'FAltimo alimento)}\par
801                 errorPlatoWasEmpty = {\cf17 false};\par
802                 {\cf19 if}(pesoLastAlimento != 0.0) pesoPlato = pesoLastAlimento; {\cf20 // Incluir \'FAltimo alimento en el 'pesoPlato' para saber 'pesoARetirar'.}\par
803                 pesoPlato += platoActual.getPesoPlato();                  {\cf20 // 'pesoPlato' puede tener inicialmente el 'pesoLastAlimento' (l\'EDnea anterior) o estar vac\'EDo (0.0). }\par
804                 comidaActual.deletePlato(platoActual);                    {\cf20 // Borrar plato actual}\par
805                 platoActual.restorePlato();                               {\cf20 // "Reiniciar" platoActual para usarlo de nuevo.}\par
806             \}\par
807             {\cf19 else}\{   {\cf20 // PLATO VAC\'CDO ==> NO HAY QUE BORRAR, PERO PUEDE QUE S\'CD RETIRAR (\'FAltimo alimento)}\par
808                 {\cf19 if}(pesoLastAlimento != 0.0)\{ {\cf20 // \'DALTIMO ALIMENTO ==> ALGO QUE RETIRAR}\par
809                     errorPlatoWasEmpty = {\cf17 false};       {\cf20 // Para mostrar mensaje de plato borrado y pedir retirarlo, aunque no sea cierto porque no se ha guardado el}\par
810                                                       {\cf20 // \'FAltimo alimento (solo su peso) y el objeto Plato est\'E1 realmente vac\'EDo.}\par
811                     pesoPlato = pesoLastAlimento;     {\cf20 // Incluir \'FAltimo alimento en el 'pesoPlato' para saber 'pesoARetirar'.}\par
812                 \}\par
813                 {\cf19 else}\{   {\cf20 // PLATO VAC\'CDO Y NO HAY \'DALTIMO ALIMENTO ==> NADA QUE RETIRAR }\par
814                     errorPlatoWasEmpty = {\cf17 true};\par
815                 \}\par
816             \}\par
817 \par
818 \par
819             {\cf20 /* -----  INFORMACI\'D3N MOSTRADA  ---------------------------------- */}\par
820             {\cf19 if}(!errorPlatoWasEmpty)\{   {\cf20 // ==> Si la comida no estaba vac\'EDa (incluyendo \'FAltimo alimento) y se ha borrado }\par
821                 showAccionConfirmada(2);     {\cf20 // Mostrar confirmacion de haber eliminado el plato}\par
822             \}\par
823             {\cf19 else}\{     {\cf20 // ==> Si el plato est\'E1 vac\'EDo, no se borra}\par
824                 showWarning(2); {\cf20 // Mostrar aviso de plato vac\'EDo y no se borra}\par
825                 Serial.println(F({\cf22 "No se ha borrado el plato porque est\'E1 vac\'EDo"}));\par
826                 previousTime = millis();   {\cf20 // Reiniciar "temporizador" de 3 segundos para, tras NO borrar, regresar a STATE_groupA / B, seg\'FAn el \'FAltimo grupo escogido.}\par
827             \}\par
828             {\cf20 /* ----- FIN INFO MOSTRADA ---------------------------------------- */}\par
829 \par
830 \par
831         \} {\cf20 // FIN if(state_prev != STATE_deleted)}\par
832         {\cf19 else} {\cf19 if}(tarado)\{                                            {\cf20 // ==> Si se viene del propio STATE_deleted, donde se puede haber tarado.}\par
833             tarado = {\cf17 false};                                         {\cf20 // Desactivar flag de haber 'tarado'.        }\par
834         \}\par
835         {\cf20 // ----------- FIN DELETE ACTUAL --------------------------------------------}\par
836 \par
837 \par
