93 {\cf18 void} checkBascula()\{\par
94     {\cf17 static} {\cf18 float} newWeight = 0.0;\par
95     {\cf17 static} {\cf18 float} lastWeight = 0.0;\par
96     \par
97     {\cf19 if} (pesado)\{\par
98         pesoARetirar = pesoRecipiente + pesoPlato;\par
99 \par
100         {\cf20 // ------- FORZAR REGRESO A INI EN CASOS ESPECIALES ------------------}\par
101         {\cf20 // Esto es necesario porque al volver a INI se tara, marcando eventoBascula = TARAR, por lo que se}\par
102         {\cf20 // debe forzar la liberaci\'F3n (eventoBascula = LIBERAR) para, tras guardar o no la comida (STATE_saved), }\par
103         {\cf20 // o borrar o no plato (STATE_deleted), se pueda regresar a INI. }\par
104         {\cf20 // Si no se hace esto, se queda atascado esperando una liberaci\'F3n que no va a llegar porque no hay }\par
105         {\cf20 // cambios en el peso (diffWeight > 2.0). }\par
106         {\cf20 /*if(((state_actual == STATE_saved) or (state_actual == STATE_deleted)) and (state_actual == STATE_Empty))\{ //Solo si se ha querido guardar desde el STATE_Empty }\par
107 {\cf20             Serial.print(F("\\nLIBERADA sola"));}\par
108 {\cf20             eventoBascula = LIBERAR;}\par
109 {\cf20             addEventToBuffer(eventoBascula);}\par
110 {\cf20             flagEvent = true;}\par
111 {\cf20         \}*/} {\cf20 // ------------------------------------------------------------------}\par
112 \par
113             \par
114         lastWeight = newWeight;\par
115         newWeight = weight;\par
116         diffWeight = abs(lastWeight - newWeight);\par
117         {\cf19 if} (diffWeight > 2.0)\{ {\cf20 // Cambio no causado por "interferencias" de la b\'E1scula}\par
118             \par
119             {\cf20 /* 'pesoBascula' representa el peso evitando peque\'F1os saltos en las medidas.}\par
120 {\cf20                Este valor es el que se usar\'E1 como peso de los alimentos.}\par
121 {\cf20                Hasta ahora se usaba 'weight' directamente, pero al a\'F1adir un nuevo alimento}\par
122 {\cf20                al plato se tomaba el \'FAltimo valor de 'weight', que podr\'EDa haber variado algo }\par
123 {\cf20                respecto a lo que se mostraba por sucesivas medidas de la b\'E1scula. */}\par
124                \par
125             pesoBascula = newWeight;\par
126             {\cf19 if}(pesoBascula < 1.0) pesoBascula = 0.0; {\cf20 //fixPesoBascula}\par
127 \par
128             {\cf19 if}(lastWeight < newWeight)\{ {\cf20 //INCREMENTO}\par
129                 Serial.println(F({\cf22 "\\nIncremento..."}));\par
130                 {\cf19 if}(lastWeight > -1.0)\{ {\cf20 //Incremento en positivo (salvando valores cercanos a 0)}\par
131                     Serial.print(F({\cf22 "\\nINCREMENTO"}));\par
132                     eventoBascula = INCREMENTO;\par
133                 \}\par
134                 {\cf19 else} {\cf19 if}(lastWeight <= -1.0)\{ {\cf20 //Incremento por tara viniendo de negativo}\par
135                     Serial.print(F({\cf22 "\\nTARADO"}));\par
136                     eventoBascula = TARAR;\par
137                 \}\par
138             \}\par
139             {\cf19 else}\{ {\cf20 //DECREMENTO}\par
140                 Serial.println(F({\cf22 "\\nDecremento..."}));\par
141                 {\cf19 if}(newWeight >= 1.0)\{ {\cf20 //Se ha quitado algo pero no todo (sigue positivo)}\par
142                     Serial.print(F({\cf22 "\\nDECREMENTO"}));\par
143                     eventoBascula = DECREMENTO;\par
144                 \}\par
145                 {\cf19 else} {\cf19 if}(newWeight < 1.0)\{ {\cf20 //peso negativo o cercano a 0.0}\par
146                     {\cf19 if}(tarado)\{ {\cf20 //Decremento debido a tara}\par
147                         Serial.print(F({\cf22 "\\nTARADO"}));\par
148                         eventoBascula = TARAR;\par
149                     \}\par
150                     {\cf19 else}\{ {\cf20 //Decremento por retirar objeto}\par
151                         {\cf19 if}(abs(abs(newWeight) - pesoARetirar) < 5.0)\{ {\cf20 //Nuevo peso (negativo) es contrario (-X = +X) al peso del plato + recipiente ==> se ha quitado todo}\par
152                             {\cf20 // Se ha puesto un umbral de 5 gr para saber si se ha retirado todo, pero podr\'EDa reducirse a 1 gr}\par
153                             Serial.print(F({\cf22 "\\nLIBERADA"}));\par
154                             eventoBascula = LIBERAR;\par
155                         \}\par
156                         {\cf19 else}\{ {\cf20 //Se est\'E1n quitando elementos de la b\'E1scula tras haber tarado, por eso se baja a negativo. Se pasa por 'QUITAR' antes de 'LIBERAR'.}\par
157                             Serial.print(F({\cf22 "\\nQUITANDO"}));\par
158                             eventoBascula = QUITAR;\par
159                         \}\par
160                     \}\par
161                 \}\par
162             \}\par
163 \par
164             Serial.println(F({\cf22 "\\n--------------------------------------"}));\par
165             Serial.print(F({\cf22 "\\nPeso anterior: "})); Serial.println(lastWeight); \par
166             Serial.print(F({\cf22 "Peso actual: "})); Serial.println(newWeight); \par
167             Serial.print(F({\cf22 "Peso Bascula: "})); Serial.println(pesoBascula);\par
168             Serial.print(F({\cf22 "Peso a retirar: "})); Serial.println(pesoARetirar);\par
169             Serial.print(F({\cf22 "Peso recipiente: "})); Serial.println(pesoRecipiente);\par
170             Serial.print(F({\cf22 "Peso plato: "})); Serial.println(pesoPlato);\par
171             Serial.println(F({\cf22 "\\n--------------------------------------"}));\par
172 \par
173             addEventToBuffer(eventoBascula);\par
174             flagEvent = {\cf17 true};\par
175         \}\par
176         pesado = {\cf17 false};\par
177     \}\par
178 \par
179 \par
180 \}\par
181 \par
182 {\cf21 #endif}\par
}
