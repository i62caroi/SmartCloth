116 {\cf18 void} writeHeaderFileSD()\{\par
117     Serial.print(F({\cf22 "\\n Creando fichero "})); Serial.print(fileCSV); Serial.println(F({\cf22 " ...\\n"}));\par
118 \par
119     {\cf20 // Debe separarse por ';' para que Excel abra el fichero csv separando las}\par
120     {\cf20 // columnas directamente:}\par
121 \par
122     String header = {\cf22 "fecha;hora;carb;carb_R;lip;lip_R;prot;prot_R;kcal;peso"};\par
123 \par
124     myFile = SD.open(fileCSV, FILE_WRITE);    {\cf20 // Todo se va a ir guardando en el mismo fichero ==> 'fileCSV' en Variables.h}\par
125     {\cf19 if} (myFile)\{\par
