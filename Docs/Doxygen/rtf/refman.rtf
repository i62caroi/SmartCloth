194 uint8_t getLastSunday(uint8_t month, uint16_t year) \{\par
195   {\cf20 // Obtener el d\'EDa m\'E1ximo del mes}\par
196   uint8_t lastDay = 31;\par
197   {\cf19 if} (month == 4 || month == 6 || month == 9 || month == 11) \{ {\cf20 // Abril, Junio, Septiembre y Noviembre}\par
198     lastDay = 30;\par
199   \} {\cf19 else} {\cf19 if} (month == 2) \{ {\cf20 // Febrero}\par
200     {\cf19 if} ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)) \{ {\cf20 // A\'F1o bisiesto}\par
201       lastDay = 29;\par
202     \} {\cf19 else} \{\par
203       lastDay = 28;\par
204     \}\par
205   \}\par
206 \par
207   {\cf20 // Obtener el DOW del \'FAltimo d\'EDa del mes}\par
208   uint8_t lastDOW = getDOW(lastDay, month, year);\par
209 \par
210   {\cf20 // Calcular el \'FAltimo domingo restando el DOW del \'FAltimo d\'EDa al d\'EDa m\'E1ximo del mes:}\par
211     {\cf20 // Por ejemplo, si el d\'EDa 31 de marzo es jueves (d\'EDa 4), entonces 31-4 = 27. El \'FAltimo}\par
212     {\cf20 // domingo es el d\'EDa 27.}\par
213     {\cf20 // En el caso en que el \'FAltimo d\'EDa del mes (p.ej. 31) caiga en Domingo, entonces se }\par
214     {\cf20 // devuelve ese d\'EDa (31).}\par
215   {\cf19 if} (lastDOW == 7) {\cf19 return} lastDay;\par
216   {\cf19 else} {\cf19 return} lastDay - lastDOW;\par
217 \par
218 \}\par
