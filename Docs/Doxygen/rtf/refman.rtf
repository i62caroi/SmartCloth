177 {\cf18 void} getAcumuladoHoyFromSD()\{\par
178 \par
179     {\cf18 char} *today = rtc.getDateStr(); {\cf20 // Es posible que se cambie de d\'EDa durante el cocinado?? Si no, hacer en setupRTC(). }\par
180 \par
181     {\cf20 // SUMAS}\par
182     {\cf18 float} sumCarb = 0.0, sumLip = 0.0, sumProt = 0.0, sumKcal = 0.0, sumPeso = 0.0;    \par
183     {\cf18 int} sumCarb_R = 0, sumLip_R = 0, sumProt_R = 0;\par
184     {\cf18 int} nComidas = 0;\par
185 \par
186     {\cf18 char} lineBuffer[128];  \par
187 \par
188     {\cf18 char} *token;\par
189     {\cf18 int} fieldIndex;\par
190 \par
191     {\cf18 bool} msg = {\cf17 true};\par
192 \par
193 \par
194     myFile = SD.open(fileCSV, FILE_READ);\par
195     {\cf19 if} (myFile)\{\par
196         {\cf19 while} (myFile.available()) \{\par
197             \par
198             myFile.readBytesUntil({\cf23 '\\n'}, lineBuffer, {\cf17 sizeof}(lineBuffer) - 1); {\cf20 // Leer l\'EDnea completa hasta el tama\'F1o m\'E1ximo del b\'FAfer}\par
199             lineBuffer[{\cf17 sizeof}(lineBuffer) - 1] = {\cf23 '\\0'}; {\cf20 // Asegurar terminaci\'F3n nula}\par
200 \par
201             {\cf20 //Serial.println(lineBuffer);}\par
202             \par
203             token = strtok(lineBuffer, {\cf22 ";"}); {\cf20 // Separar campos de la l\'EDnea utilizando el delimitador ';'}\par
204             fieldIndex = 0;\par
205 \par
206             {\cf19 if}(strcmp(today, token) == 0)\{ {\cf20 // today == primer token ==> comida guardada hoy}\par
207                 \par
208                 {\cf19 if}(msg)\{\par
209                     Serial.println(F({\cf22 "Obteniendo Acumulado Hoy..."}));\par
210                     msg = {\cf17 false}; {\cf20 // Solo imprimir una vez y si hay algo que sumar}\par
211                 \}\par
212 \par
213                 nComidas++; {\cf20 // Incrementar numero comidas guardadas hoy}\par
214 \par
215                 {\cf18 float} valueFloat;\par
216                 {\cf18 int} valueInt;\par
217 \par
218                 {\cf19 while} (token != NULL) \{\par
219 \par
220                     {\cf19 switch} (fieldIndex)\{ {\cf20 // fieldIndex = 0 => fecha     fieldIndex = 1 => hora}\par
221                         {\cf19 case} 2:   valueFloat = atof(token);     sumCarb   += valueFloat;     {\cf19 break};    {\cf20 // Carbohidratos}\par
222                         {\cf19 case} 3:   valueInt   = atoi(token);     sumCarb_R += valueInt;       {\cf19 break};    {\cf20 // Raciones de carbohidratos}\par
223                         {\cf19 case} 4:   valueFloat = atof(token);     sumLip    += valueFloat;     {\cf19 break};    {\cf20 // L\'EDpidos (Grasas)}\par
224                         {\cf19 case} 5:   valueInt   = atoi(token);     sumLip_R  += valueInt;       {\cf19 break};    {\cf20 // Raciones de l\'EDpidos}\par
225                         {\cf19 case} 6:   valueFloat = atof(token);     sumProt   += valueFloat;     {\cf19 break};    {\cf20 // Prote\'EDnas}\par
226                         {\cf19 case} 7:   valueInt   = atoi(token);     sumProt_R += valueInt;       {\cf19 break};    {\cf20 // Raciones de prote\'EDnas}\par
227                         {\cf19 case} 8:   valueFloat = atof(token);     sumKcal   += valueFloat;     {\cf19 break};    {\cf20 // Kilocalor\'EDas}\par
228                         {\cf19 case} 9:   valueFloat = atof(token);     sumPeso   += valueFloat;     {\cf19 break};    {\cf20 // Peso}\par
229                     \}\par
230 \par
231                     {\cf20 // Obtener el siguiente campo}\par
232                     token = strtok(NULL, {\cf22 ";"});\par
233                     fieldIndex++;\par
234                 \}\par
235             \}\par
236          \par
237         \}\par
238 \par
239         myFile.close();\par
240 \par
241 \par
242         {\cf20 // ----- ACTUALIZAR ACUMULADO HOY -----}\par
243         ValoresNutricionales valAux(sumCarb, sumLip, sumProt, sumKcal);   \par
244         diaActual.setValoresDiario(valAux);                               {\cf20 // Inicializar valores nutricionales del Acumulado Hoy}\par
245         diaActual.setPesoDiario(sumPeso);                                 {\cf20 // Actualizar peso del Acumulado Hoy}\par
246         diaActual.setNumComidas(nComidas);                                {\cf20 // Actualizar n\'BA de comidas del Acumulado Hoy}\par
247         \par
248     \}\par
249     {\cf19 else}\{\par
250         Serial.println({\cf22 "Error opening file for reading!"});\par
251     \}\par
252 \par
253 \}\par
254 \par
255 \par
256 \par
257 {\cf21 #endif}\par
