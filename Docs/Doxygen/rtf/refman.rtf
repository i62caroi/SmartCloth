378 {\cf18 bool} checkStateConditions()\{\par
379     {\cf19 for} ({\cf18 int} i = 0; i < RULES; i++)\{\par
380         {\cf19 if}(rules[i].state_i == state_actual)\{\par
381             {\cf19 if}(rules[i].condition == lastEvent)\{    \par
382                 state_new = rules[i].state_j;     {\cf20 // Nuevo estado}\par
383                 doneState = {\cf17 false};                {\cf20 // Desactivar flag de haber hecho las actividades del estado}\par
384                 {\cf19 return} {\cf17 true};                        \par
385             \}\par
386         \}\par
387     \}\par
388     {\cf19 return} {\cf17 false};                                 {\cf20 // Si no se ha cumplido ninguna regla de transici\'F3n ==> ERROR DE EVENTO}\par
